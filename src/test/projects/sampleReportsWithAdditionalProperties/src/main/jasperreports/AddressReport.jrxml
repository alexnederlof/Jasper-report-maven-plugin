<?xml version="1.0" encoding="UTF-8"?>


<jasperReport
		xmlns="http://jasperreports.sourceforge.net/jasperreports"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
		name="AddressReport" pageWidth="175" pageHeight="842" columnWidth="175" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<style name="Sans_Normal" isDefault="true" fontName="DejaVu Sans" fontSize="8" isBold="false" isItalic="false" isUnderline="false" isStrikeThrough="false"/>
	<style name="Sans_Bold" isDefault="false" fontName="DejaVu Sans" fontSize="8" isBold="true" isItalic="false" isUnderline="false" isStrikeThrough="false"/>
	<style name="Sans_Italic" isDefault="false" fontName="DejaVu Sans" fontSize="8" isBold="false" isItalic="true" isUnderline="false" isStrikeThrough="false"/>
	<parameter name="City" class="java.lang.String">
	</parameter>
	<queryString><![CDATA[SELECT * FROM Address WHERE City = $P{City}]]></queryString>
	<field name="Id" class="java.lang.Integer">
	</field>
	<field name="FirstName" class="java.lang.String">
	</field>
	<field name="LastName" class="java.lang.String">
	</field>
	<field name="Street" class="java.lang.String">
	</field>
	<title>
		<band height="14">
			<staticText>
				<reportElement x="0" y="2" width="20" height="10" style="Sans_Italic"/>
				<text><![CDATA[Title]]></text>
			</staticText>
			<textField>
				<reportElement x="20" y="2" width="155" height="10" style="Sans_Bold"/>
				<textElement textAlignment="Center"/>
				<textFieldExpression class="java.lang.String"><![CDATA["Addresses in " + $P{City}]]></textFieldExpression>
			</textField>
		</band>
	</title>
	<pageHeader>
		<band height="14">
			<staticText>
				<reportElement x="0" y="2" width="175" height="10" forecolor="#8080" style="Sans_Italic"/>
				<box>
					<pen lineWidth="0.5"/>
				</box>
				<text><![CDATA[Page Header]]></text>
			</staticText>
		</band>
	</pageHeader>
	<columnHeader>
		<band height="14">
			<frame>
				<reportElement x="0" y="2" width="175" height="10" forecolor="#ccffff" backcolor="#ccffff"/>
				<box>
					<pen lineWidth="1"/>
				</box>
				<staticText>
					<reportElement mode="Opaque" x="0" y="0" width="20" height="10" backcolor="#ccffff" style="Sans_Bold"/>
					<textElement textAlignment="Center"/>
					<text><![CDATA[ID]]></text>
				</staticText>
				<staticText>
					<reportElement mode="Opaque" x="20" y="0" width="85" height="10" backcolor="#ccffff" style="Sans_Bold"/>
					<text><![CDATA[Name]]></text>
				</staticText>
				<staticText>
					<reportElement mode="Opaque" x="105" y="0" width="70" height="10" backcolor="#ccffff" style="Sans_Bold"/>
					<text><![CDATA[Street]]></text>
				</staticText>
			</frame>
		</band>
	</columnHeader>
	<detail>
		<band height="14">
			<textField>
				<reportElement x="0" y="2" width="15" height="10"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.Integer"><![CDATA[$F{Id}]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="20" y="2" width="80" height="10"/>
				<textElement/>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{FirstName} + " " + $F{LastName}]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="105" y="2" width="70" height="10"/>
				<textElement/>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{Street}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
	<columnFooter>
		<band height="14">
			<staticText>
				<reportElement x="0" y="2" width="175" height="10" style="Sans_Italic"/>
				<box>
					<pen lineWidth="0.5"/>
				</box>
				<text><![CDATA[Column Footer]]></text>
			</staticText>
		</band>
	</columnFooter>
	<pageFooter>
		<band height="14">
			<frame>
				<reportElement mode="Transparent" x="0" y="2" width="175" height="10" forecolor="#8080"/>
				<box>
					<pen lineWidth="0.5"/>
				</box>
				<staticText>
					<reportElement x="0" y="0" width="60" height="10" forecolor="#8080" style="Sans_Italic"/>
					<text><![CDATA[Page Footer]]></text>
				</staticText>
				<textField>
					<reportElement x="100" y="0" width="50" height="10" forecolor="#8080" style="Sans_Italic"/>
					<textElement textAlignment="Right"/>
					<textFieldExpression class="java.lang.String"><![CDATA["Page " + String.valueOf($V{PAGE_NUMBER}) + " of "]]></textFieldExpression>
				</textField>
				<textField evaluationTime="Report">
					<reportElement x="150" y="0" width="20" height="10" forecolor="#8080" style="Sans_Italic"/>
					<textFieldExpression class="java.lang.Integer"><![CDATA[$V{PAGE_NUMBER}]]></textFieldExpression>
				</textField>
			</frame>
		</band>
	</pageFooter>
	<summary>
		<band height="14">
			<frame>
				<reportElement x="0" y="2" width="175" height="10" forecolor="#33cccc" backcolor="#33cccc"/>
				<staticText>
					<reportElement mode="Opaque" x="0" y="0" width="100" height="10" backcolor="#33cccc" style="Sans_Italic"/>
					<text><![CDATA[Summary]]></text>
				</staticText>
				<staticText>
					<reportElement mode="Opaque" x="100" y="0" width="50" height="10" backcolor="#33cccc" style="Sans_Bold"/>
					<textElement textAlignment="Right"/>
					<text><![CDATA[Count :]]></text>
				</staticText>
				<textField>
					<reportElement mode="Opaque" x="150" y="0" width="25" height="10" backcolor="#33cccc" style="Sans_Bold"/>
					<textElement textAlignment="Right"/>
					<textFieldExpression class="java.lang.Integer"><![CDATA[$V{REPORT_COUNT}]]></textFieldExpression>
				</textField>
			</frame>
		</band>
	</summary>
</jasperReport>
